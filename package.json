{
  "name": "solid-start-preload",
  "version": "0.1.1",
  "description": "SSR Preload CSS, scripts and assets in Solid Start",
  "repository": {
    "type": "git",
    "url": "https://github.com/shiro/solid-start-preload"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index/index.module.js",
  "module": "./dist/index/index.module.js",
  "exports": {
    ".": {
      "types": "./dist/index/index.d.ts",
      "import": "./dist/index/index.module.js",
      "solid": "./dist/source/my-lib.jsx",
      "browser": "./dist/index/index.js",
      "require": "./dist/index/index.js",
      "node": "./dist/index/index.js"
    },
    "./server": {
      "types": "./dist/server/server.d.ts",
      "import": "./dist/server/server.module.js",
      "solid": "./dist/source/my-lib.jsx",
      "browser": "./dist/server/server.js",
      "require": "./dist/server/server.js",
      "node": "./dist/server/server.js"
    },
    "./babel": {
      "types": "./dist/babel/babel.d.ts",
      "import": "./dist/babel/babel.module.js",
      "solid": "./dist/source/my-lib.jsx",
      "browser": "./dist/babel/babel.js",
      "require": "./dist/babel/babel.js",
      "node": "./dist/babel/babel.js"
    }
  },
  "scripts": {
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\"",
    "build": "rollup -c rollup.config.js",
    "test": "vitest run",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@solidjs/start": "1.0.0-rc.0",
    "@types/node": "20.12.7",
    "prettier": "3.2.5",
    "rollup-preset-solid": "2.0.1",
    "solid-js": "*",
    "typescript": "5.4.5",
    "@solidjs/router": "0.13.2",
    "vinxi": "0.3.*",
    "vitest": "1.5.0"
  }
}
